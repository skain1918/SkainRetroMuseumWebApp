@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<table class="table table-bordered">
    @foreach (AppUser user in Model)
            {
        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
        </tr>

    }
</table>